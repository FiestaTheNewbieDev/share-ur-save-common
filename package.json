{
  "name": "share-ur-save-common",
  "version": "1.0.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./client": {
      "require": "./dist/cjs/client/index.js",
      "import": "./dist/mjs/client/index.js",
      "types": "./dist/esm/client/index.d.ts"
    },
    "./misc": {
      "require": "./dist/cjs/misc/index.js",
      "import": "./dist/mjs/misc/index.js",
      "types": "./dist/esm/misc/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "migrate": "scripts/migrate.js"
  },
  "scripts": {
    "build": "scripts/build.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiestaTheNewbieDev/share-ur-save-common"
  },
  "keywords": [],
  "author": "",
  "private": false,
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "prisma": "^5.21.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  }
}
